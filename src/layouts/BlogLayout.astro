---
import Layout from "./Layout.astro";

const { frontmatter } = Astro.props;
---

<Layout title={frontmatter.title}>
  <h1 class="text-2xl dark:text-muted-foreground self-start font-serif">
    {frontmatter.title}
  </h1>
  <div
    class="flex dark:text-muted-foreground flex-col font-serif text-base mt-8"
  >
    <slot />
  </div>
</Layout>
